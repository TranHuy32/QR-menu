{
  "name": "backend",
  "version": "1.0.0",
  "description": "food ordering app ",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "create-migration": "npx sequelize-cli migration:generate --name",
    "undo-migrate": "babel-node ./node_modules/.bin/sequelize db:migrate:undo",
    "migrate": "babel-node node_modules/.bin/sequelize db:migrate",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && node ./build/src/index.js",
    "start": "nodemon --exec ./node_modules/.bin/babel-node ./index.js"
  },
  "author": "HTH",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mysql2": "^3.10.1",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/node": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2"
  }
}
